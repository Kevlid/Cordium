{
    "name": "cordium",
    "version": "0.0.0",
    "description": "Framework for creating Discord applications",
    "author": "Kevlid",
    "license": "MIT",
    "keywords": [
        "typescript",
        "discord",
        "framework",
        "commands",
        "events"
    ],
    "scripts": {
        "build": "tsup",
        "dev": "tsup --watch",
        "clean": "rm -rf ./dist"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Kevlid/Cordium.git"
    },
    "main": "dist/cjs/index.js",
    "module": "dist/esm/index.mjs",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/esm/index.mjs",
            "require": "./dist/cjs/index.js"
        }
    },
    "files": [
        "dist"
    ],
    "devDependencies": {
        "@types/node": "^24.6.2",
        "tsup": "^8.0.0",
        "typescript": "^5.0.0"
    },
    "engines": {
        "node": ">=16"
    },
    "packageManager": "pnpm@10.18.2",
    "dependencies": {
        "discord.js": "^14.22.1"
    }
}
